{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "databricks25-course-adf"
		},
		"ls_formula1dl_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_formula1dl_storage'"
		},
		"ls_databricks_project_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0725-231538-phhapd4e"
		},
		"ls_formula1dl_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1projectdls.dfs.core.windows.net/"
		},
		"tr_process_f1_data_properties_pl_process_formula1_data_parameters_p_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databricks25-course-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_formula1dl_storage_accountKey": {
						"value": "[parameters('ls_formula1dl_storage_accountKey')]"
					},
					"ls_databricks_project_ws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_project_ws_properties_typeProperties_existingClusterId')]"
					},
					"ls_formula1dl_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_formula1dl_storage_properties_typeProperties_url')]"
					},
					"tr_process_f1_data_properties_pl_process_formula1_data_parameters_p_window_end_date": {
						"value": "[parameters('tr_process_f1_data_properties_pl_process_formula1_data_parameters_p_window_end_date')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}